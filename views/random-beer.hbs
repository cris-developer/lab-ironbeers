<!--Iteration 3.2 random-beer.hbs-->
<div>
    {{#each responseFromAPI}}
    <div>
        <img src={{this.image_url}}>
        <h1>{{this.name}}</h1>
        <h2>Description</h2>
        <p>{{this.description}}</p>
        <h2>Tagline</h2>
        <p>{{this.tagline}}</p>
        <h2>Food pairing</h2>
        <h2>Brewers tips</h2>
        <p>{{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div>
